<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>

	<!-- Parent POM contains all general settings for building and releasing of all JEAF projects. -->
	<parent>
		<groupId>com.anaptecs.jeaf</groupId>
		<artifactId>jeaf-project-parent</artifactId>
		<version>1.2.5</version>
	</parent>
	
	<artifactId>jeaf-plugin-project-parent</artifactId>
	<version>1.13.0</version>


	<packaging>pom</packaging>
	<name>JEAF Plugin Project Parent</name>
	<description>Maven project defines properties, settings and version of libraries, plugins and modules that are used by all JEAF plugin projects.</description>
	<url>https://www.anaptecs.de/jeaf</url>

	<scm>
		<connection>scm:git:https://github.com/anaptecs/jeaf-plugin-project-parent.git</connection>
		<developerConnection>scm:git:https://github.com/anaptecs/jeaf-plugin-project-parent.git</developerConnection>
		<url>https://github.com/anaptecs/jeaf-plugin-project-parent.git</url>
		<tag>v1.13.0</tag>
	</scm>

	<properties>
		<!-- Versions of dependent libraries -->
		<junit.version>5.9.3</junit.version>
		<junit.platform.version>1.9.3</junit.platform.version>
		<junit3.version>3.8.2</junit3.version>

		<!-- Logging frameworks -->
		<commons-logging.version>1.2</commons-logging.version>
		<log4j.version>2.20.0</log4j.version>
		<slf4j-api.version>1.7.36</slf4j-api.version>
		<slf4j-log4j.version>1.7.36</slf4j-log4j.version>
		
		<commons-lang.version>3.14.0</commons-lang.version>
		<commons-io.version>2.16.1</commons-io.version>

		<!-- JEE libraries -->
		<javaee-api.version>8.0.1</javaee-api.version>
		<servlet-api.version>3.1.0</servlet-api.version>
		<jpa2-api.version>1.0.1.Final</jpa2-api.version>
		<java.mail.version>1.6.0</java.mail.version>
		<javax.el.version>3.0.1-b11</javax.el.version>
		<javax.ws.rs.version>2.1</javax.ws.rs.version>
		<javax.annotation-api.version>1.3.2</javax.annotation-api.version>

		<!-- Maven Plugin libraries -->
		<maven.plugin.api.version>3.8.6</maven.plugin.api.version>
		<maven.core.jar.version>3.8.5</maven.core.jar.version>
		<maven.plugin.annotations.version>3.8.2</maven.plugin.annotations.version>
		<mojo-executor.version>2.4.0</mojo-executor.version>

		<!-- openArchitecture Ware oAW -->
		<oaw.version>4.3.1</oaw.version>

		<!-- Classgraph -->
		<classgraph.version>4.8.160</classgraph.version>
		
		<swagger.annotations.version>2.2.14</swagger.annotations.version>
		<swagger.annotations.v2.version>1.6.11</swagger.annotations.v2.version>
		
		<apache.httpclient5.version>5.2.1</apache.httpclient5.version>
		
		<!-- Resilience4J -->
		<resilience4j.version>1.7.1</resilience4j.version>
		
		<jackson.version>2.15.2</jackson.version>
		<jackson.databind.nullable.version>0.2.6</jackson.databind.nullable.version>
		<validation-api.version>2.0.1.Final</validation-api.version>
		<apache.commons-lang.version>3.14.0</apache.commons-lang.version>
		<apache.commons.io.version>2.16.1</apache.commons.io.version>
		<apache.poi.version>5.3.0</apache.poi.version>
		<xml.unit.version>2.9.1</xml.unit.version>


		
		<!-- Mock Server Version -->
		<mock.server.version>5.15.0</mock.server.version>
	</properties>

</project>